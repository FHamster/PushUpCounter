            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="180.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <StackPane prefHeight="150.0" prefWidth="150.0" style="-fx-border-color: #FBA5B0;" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <children>
                  <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: FBA5B0;">
                     <children>
                        <ImageView fitHeight="128.0" fitWidth="128.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
                           <image>
                              <Image url="@../img/icon.jpg" />
                           </image>
                        </ImageView>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </StackPane>
               </children></StackPane>
            <Text styleClass="TextStyle" text="猪猪做了" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
            <Text fx:id="SelectView" style="-fx-font-size: 40;" styleClass="TextStyle" text="5432" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
            <Text styleClass="TextStyle" text="个俯卧撑" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.rowIndex="3" />
            <Text fx:id="TimeView" styleClass="TextStyle" text="2019年01月13日" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <Button id="SelectBtn" fx:id="SelectBtn" contentDisplay="TEXT_ONLY" mnemonicParsing="false" text="查看记录" textFill="WHITE" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
            <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
               <children>
                  <Button id="CloseBtn" fx:id="CloseBtn" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#closeSystem" />
